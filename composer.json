{
    "name": "root/mlight-framework",
    "description": "Small lightweight PHP framework",
    "type": "project",
    "require": {
        "php": ">=5.4.0",
        "doctrine/orm": "^2.5",
        "npm-asset/jquery": "^3.2.1",
        "npm-asset/underscore": "^1.8.3",
        "npm-asset/backbone": "^1.3.3",
        "bower-asset/materialize": "^0.99.0",
        "npm-asset/backbone.marionette": "^3.3.1",
        "npm-asset/lodash": "^4.17.4"
    },
    "require-dev": {
        "codeception/codeception": "^2.3",
        "npm-asset/mocha": "^3.2.0"
    },
    "config": {
        "fxp-asset": {
            "installer-paths": {
                "npm-asset-library": "vendor/npm",
                "bower-asset-library": "vendor/bower"
            }
        }
    },
    "license": "GPLv3",
    "authors": [
        {
            "name": "socialist",
            "email": "serega@socialist.pw"
        }
    ],
    "autoload": {
        "psr-4": {
            "core\\": "core/",
            "application\\": "application/"
        }
    }
}
